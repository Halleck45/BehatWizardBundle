{
    "name": "halleck45/behat-wizard-bundle",
    "description": "This bundle provides a graphical interface to manage your features",
    "keywords": ["behat", "gui"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
    {
        "name": "Jean-François Lépine",
        "homepage": "http://blog.lepine.pro"
    },
    {
        "name": "Karol Sójko",
        "homepage": "http://blog.karolsojko.com"
    }
    ],
    "require": {
        "php":                           ">=5.3.3",
        "symfony/symfony":               "*@stable",
        "sensio/framework-extra-bundle": "*@dev",
        "halleck45/behat-tools-bundle":  "dev-master",
        "symfony/assetic-bundle": "2.1.*",
        "kriswallsmith/assetic": "1.1.*@dev"

    },
    "require-dev": {
        "behat/behat":                  "*@stable",
        "behat/symfony2-extension":     "*@stable",
        "behat/mink-extension":         "*",
        "behat/mink-browserkit-driver": "*",
        "behat/sahi-client": "v1.0.1",
        "behat/mink-sahi-driver": "*",
        "behat/gherkin":            "@dev"
    },
    "autoload": {
        "psr-0": {
            "Hal\\Bundle\\BehatWizard": ""
        }
    },
    "target-dir": "Hal/Bundle/BehatWizard",
    "repositories": [
    {
        "type": "git",
        "url": "git://github.com/Halleck45/Gherkin.git"
    }
    ]
}
